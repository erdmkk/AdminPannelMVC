@using EntityLayer
@model List<Product>


 <table id="example" class="table table-dark table-striped" style="width:100%">
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Description</th>
			<th>Price</th>
			<th>Stock</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var a in Model)
		{
			<tr>
				<td>@a.id</td>
				<td>@a.name</td>
				<td>@a.description</td>
				<td>@a.price</td>
				<td>@a.stock</td>
			</tr>
		}
	</tbody>
</table>